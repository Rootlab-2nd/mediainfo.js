{
  "globalSetup": "<rootDir>/__tests__/jest/setup.ts",
  "setupFilesAfterEnv": ["<rootDir>/__tests__/jest/toBeNear.ts"],
  "testEnvironment": "./__tests__/jest/environment.ts",
  "testMatch": ["**/__tests__/**/*.test.ts"],
  "testPathIgnorePatterns": [
    "<rootDir>/build/",
    "<rootDir>/dist/",
    "<rootDir>/node_modules/",
    "<rootDir>/__tests__/.eslintrc.js"
  ],
  "verbose": true
}
